{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es2019",
    "module": "ESNext",
    "jsx": "react",
    "moduleResolution": "node",
    "noUnusedParameters": false,
    "removeComments": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "lib": ["es2015", "es2016", "dom"],
    "types": ["node"]
  }
}
