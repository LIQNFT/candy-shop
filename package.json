{
  "name": "@liqnft/candy-shop",
  "version": "0.4.14",
  "description": "Your own decentralized and escrowless marketplace in minutes",
  "author": "LIQNFT",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "dist"
  ],
  "main": "./dist/candy-shop.umd.js",
  "module": "./dist/candy-shop.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/candy-shop.es.js",
      "require": "./dist/candy-shop.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build:lib": "run-p build:scripts build:types",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias",
    "build:example": "vite build -c example-vite.config.ts",
    "preview": "vite preview -c example-vite.config.ts",
    "gh-pages": "push-dir --dir=example-dist --branch=gh-pages --cleanup --verbose",
    "deploy": "yarn build:example && yarn gh-pages",
    "format:check": "prettier --check lib",
    "format:fix": "prettier --write lib",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "@emotion/react": "^11.0.0-rc.0",
    "@emotion/styled": "^11.8.1",
    "@project-serum/anchor": "^0.23.0",
    "@solana/spl-token": "^0.2.0",
    "@solana/wallet-adapter-react": "^0.12.5",
    "@solana/wallet-adapter-wallets": "^0.11.0",
    "@solana/web3.js": "^1.37.1",
    "axios": "^0.26.1",
    "borsh": "^0.7.0",
    "buffer": "^6.0.3",
    "qs": "^6.10.3",
    "rc-notification": "^4.5.7",
    "react-app-polyfill": "^3.0.0",
    "solana-candy-shop-schema": "git+https://github.com/LIQNFT/solana-candy-shop-schema.git#d8cc8ebe62d9b532fe120cf2f5a95ab6e7ced82b"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-inject": "^4.0.4",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/wallet-adapter-ant-design": "^0.7.0",
    "@solana/wallet-adapter-base": "^0.6.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/bn.js": "^5.1.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "antd": "^4.19.1",
    "api": "^4.2.0",
    "components": "^0.1.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.8",
    "postcss-modules": "^4.3.1",
    "prettier": "^2.6.2",
    "react": ">=16.8.0",
    "react-dom": "18.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tsc-alias": "^1.6.6",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vite-tsconfig-paths": "^3.4.1"
  }
}
